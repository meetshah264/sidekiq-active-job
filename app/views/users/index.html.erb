<h1>Welcome</h1>

<table border="2">
  <thead>
  <th>Name</th>
  <th>Phone Number</th>
  <th>Edit</th>
  <th>Show</th>
  <th>Destroy</th>
  </thead>
  <% @users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= user.phone %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %>
    <td><%= link_to 'Show', user_path(user) %>
    <td><%= link_to 'Destroy', user_path(user), method: :delete %>
  </tr>
  <% end %>
</table>

<%= link_to "New User", new_user_path %>


